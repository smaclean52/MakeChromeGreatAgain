{
  "manifest_version": 2,
  "name": "MakeAmericaGreatAgain",
  "version": "1",
  "background": {
    "scripts": ["background.js"]
  },

  

 "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/"
  ],

  "content_scripts": [
    {
      "matches": ["http://www.google.com/*"],
      "js": ["content.js"],
      "css" : ["yourcss.css"],
      "run_at": "document_end" 

    }
  ],
"content_scripts": [

    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.2.2.js", "content.js"],
      "css" : ["override.css"]
    }
  ],
  "browser_action": {
    "default_icon": "logo.png"
  }
}